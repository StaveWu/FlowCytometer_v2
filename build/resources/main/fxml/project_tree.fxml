<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.VBox?>

<VBox fillWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectTree.ProjectTreeController">
    <children>
        <TreeView fx:id="treeView" root="Root" VBox.vgrow="ALWAYS" />
        <Button fx:id="btnAdd" mnemonicParsing="false" onMouseClicked="#addNode" text="Add" />
    </children>
</VBox>
