<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <ToolBar>
      <items>
           <Button fx:id="addBtn" mnemonicParsing="false" onAction="#newChannelCell" text="添加通道">
               <tooltip>
                   <Tooltip text="添加通道" />
               </tooltip>
           </Button>
         <Button fx:id="settingBtn" mnemonicParsing="false" onAction="#setting" text="设置">
            <tooltip>
               <Tooltip text="设置" />
            </tooltip>
         </Button>
      </items>
   </ToolBar>
    <ScrollPane>
        <HBox fx:id="channelsHBox" fillHeight="false" prefHeight="800.0" spacing="20">
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding></HBox>
    </ScrollPane>
    <stylesheets>
        <URL value="@channel.css" />
    </stylesheets>
</fx:root>
